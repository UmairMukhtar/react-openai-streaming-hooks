{
  "name": "react-openai-streaming-hooks",
  "version": "1.0.2",
  "description": "React Hooks for streaming connections to OpenAI APIs",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "eslint src/ --ext .ts",
    "build": "tsc",
    "clean": "rm -rf node_modules/ dist/ coverage/",
