    "example": "vite serve example",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "prettier": "prettier --write ."
  },
  "keywords": [
    "react",
    "react-hooks",
    "openai"
  ],
  "author": "UmairMukhtar",
  "contributors": [
    "Umair Mukhtar (https://github.com/UmairMukhtar)"
  ],
  "license": "MIT",
  "dependencies": {
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@testing-library/react": "^14.0.0",
    "@types/react": "^18.0.29",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/coverage-c8": "^0.31.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "jsdom": "^21.1.1",
    "prettier": "2.8.8",
    "react-dom": "^18.2.0",
    "typescript": "^5.0.2",
    "vite": "^4.2.1",
    "vitest": "^0.31.0"
  },
  "directories": {
    "example": "example",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UmairMukhtar/react-openai-streaming-hooks.git"
  },
  "bugs": {
    "url": "https://github.com/UmairMukhtar/react-openai-streaming-hooks/issues"
  },
  "homepage": "https://github.com/UmairMukhtar/react-openai-streaming-hooks#readme"
